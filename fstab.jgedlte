# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.

#<src>                                                <mnt_point>   <type>  <mnt_flags and options>                     <fs_mgr_flags>
/dev/block/platform/msm_sdcc.1/by-name/system         /system       ext4    ro,barrier=1                                wait
/dev/block/platform/msm_sdcc.1/by-name/userdata       /data         ext4    nosuid,nodev,barrier=1,noauto_da_alloc      wait,check,encryptable=footer


#
/devices/platform/msm_sdcc.2/mmc_host/mmc2          auto                    auto    default     voldmanaged=extSdCard:auto
/devices/platform/msm_hsusb_host/usb*sda            auto                    auto    default     voldmanaged=UsbDriveA:auto
/devices/platform/msm_hsusb_host/usb*sdb            auto                    auto    default     voldmanaged=UsbDriveB:auto
/devices/platform/msm_hsusb_host/usb*sdc            auto                    auto    default     voldmanaged=UsbDriveC:auto
/devices/platform/msm_hsusb_host/usb*sdd            auto                    auto    default     voldmanaged=UsbDriveD:auto
/devices/platform/msm_hsusb_host/usb*sde            auto                    auto    default     voldmanaged=UsbDriveE:auto
/devices/platform/msm_hsusb_host/usb*sdf            auto                    auto    default     voldmanaged=UsbDriveF:auto
